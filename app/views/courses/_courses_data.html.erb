<tr class="clr-slate" id= "course-row-<%= course.id %>" data-id="<%= course.id %>">
  <td>
    <input type="checkbox" id="check_all" class="hidden data-check" name="user_all" value="val" data-action="change->records-table#check change->records-table#bulkId" autocomplete="off" data-id="<%= course.id %>">
    <div class="ic-checkbox-empty w-[20px] h-[20px] hover:opacity-80" data-action="mousedown->effects#check"></div>
  </td>
  <td><a href="<%= course_path(course.id) %>" class="hover:underline"><%= course.title %></a></td>
  <td><%= course.enrollments.count > 0 ? 'Enrolled' : '---' %></td>
  <td><%= course.created_by %></td>
  <td><%= course.created_at %></td>
</tr>
